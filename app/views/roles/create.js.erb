<% if @role.errors.present? %>
  swal({
     icon: "error",
     title: "oops..!",
     buttons: false,
     timer: 3000,
      text: "<%= @role.errors.full_messages.join('.') %>",
      html: true
  });
<% else %>
  swal({
     icon: "success",
     title: "Success",
     buttons: false,
     timer: 3000,
    text: "Role created successfully"}).then(function(){
    window.location = '<%= settings_path %>'

  });
<% end %>